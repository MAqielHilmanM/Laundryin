<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.creative.iam.laundryin">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.splash.SplashActivity"></activity>
        <activity android:name=".views.order.detail.DetailPesanan" />
        <activity android:name=".views.main.UtamaActvity" />
        <activity android:name=".views.notification.NotifikasiActivity" />
        <activity android:name=".views.order.success.PesanBerhasilActvity" />
        <activity android:name=".views.paket.DetailPaketLaundry" />
        <activity android:name=".views.registrasi.SignUpActivity" />
        <activity
            android:name=".views.login.LoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>